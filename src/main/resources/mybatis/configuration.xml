<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias type="com.example.demo.vo.HospitalVo"
			alias="hospitalvo" />
		<typeAlias type="com.example.demo.vo.EmployeeVo"
			alias="employeevo" />
		<typeAlias type="com.example.demo.vo.DoctorVo"
			alias="doctorvo"/>
		<typeAlias type="com.example.demo.vo.DiseaseDataVo"
			alias="diseasedatavo"/>	
		<typeAlias type="com.example.demo.vo.MedicineDataVo"
			alias="medicinedatavo"/>
		<typeAlias type="com.example.demo.vo.PrescriptionVo"
			alias="prescriptionvo"/>
		<typeAlias type="com.example.demo.vo.DiseaseVo"
			alias="diseasevo"/>
		<typeAlias type="com.example.demo.vo.TreatmentVo"
			alias="treatmentvo"/>
		<typeAlias type="com.example.demo.vo.DiagnosisVo"
			alias="diagnosisvo"/>
		<typeAlias type="com.example.demo.vo.AppointmentVo"
			alias="appointmentvo"/>
		<typeAlias type="com.example.demo.vo.PatientVo"
			alias="patientvo"/>
		<typeAlias type="com.example.demo.vo.NurseVo"
			alias="nursevo"/>
		<typeAlias type="com.example.demo.vo.AttendanceScheduleVo"
			alias="attendanceschedulevo"/>
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/mappers/admin.xml" />
		<mapper resource="mybatis/mappers/doctor.xml" />
		<mapper resource="mybatis/mappers/nurse.xml" />
		<mapper resource="mybatis/mappers/common.xml" />
	</mappers>
</configuration>
